# Regami Staging Environment Variables
# Copy to .env and fill in actual values

# Docker image tag
IMAGE_TAG=latest

# Application
SECRET_KEY=staging-secret-key-at-least-32-chars
CORS_ORIGINS=https://staging.regami.com

# PostgreSQL
POSTGRES_USER=regami
POSTGRES_PASSWORD=staging-postgres-password
POSTGRES_DB=regami

# MinIO
MINIO_ROOT_USER=regami
MINIO_ROOT_PASSWORD=staging-minio-password
S3_PUBLIC_BASE_URL=https://storage.staging.regami.com

# Basic Auth (protect staging from public access)
BASIC_AUTH_USERNAME=staging
BASIC_AUTH_PASSWORD=your-staging-password

# Sentry (optional)
SENTRY_DSN=
